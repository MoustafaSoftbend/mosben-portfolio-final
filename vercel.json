{
    "build": {
      "env": {
        "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true",
        "CHROMIUM_PATH": "/usr/bin/chromium-browser"
      }
    },
    "functions": {
      "api/**/*": {
        "memory": 1024,
        "maxDuration": 10,
        "runtime": "nodejs16.x"
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  